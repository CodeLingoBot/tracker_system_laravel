{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0",
    "alexusmai/laravel-file-manager": "2.2.2",
    "doctrine/dbal": "2.9.2",
    "farhanwazir/laravelgooglemaps": "2.3.9",
    "jeremykenedy/laravel-roles": "1.6.0",
    "jeremykenedy/laravel-users": "3.3.0",
    "jeroennoten/laravel-adminlte": "1.25.1",
    "laravel/framework": "5.8.11",
    "laravel/tinker": "1.0.8",
    "sentry/sentry-laravel": "1.0.1"
  },
  "require-dev": {
    "fzaninotto/faker": "1.8.0",
    "mockery/mockery": "0.9.11",
    "phpunit/phpunit": "5.7.27"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
